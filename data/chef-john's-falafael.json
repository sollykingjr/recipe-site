  "ingredients": [
    "1cup dried garbanzo beans aka chickpeas",
    "1/2yellow onion, diced",
    "4cloves minced garlic",
    "1/2cup chopped Italian parsley",
    "1 1/2tsp salt, or to taste",
    "1tsp freshly ground black pepper",
    "1tsp cumin",
    "1/2tsp ground coriander",
    "1/8tsp cayenne pepper",
    "1/4tsp baking soda",
    "1rounded tablespoon flour",
    "2tsp lemon juice"
  ],
  "directions": [
    "Soak chickpeas overnight in water",
    "Pulse everything in Food Processor. Shape.",
    "Fry at 350\u00b0F for about 5 minutes or until browned and crispy"
  ]
}