{
  "title": "Hawaij",
  "source": "Shuk From Market to Table",
  "servings": "Makes about \u2153 cup (32 g)",
  "categories": [
    "Israel",
    "Shuk - From Market to Table"
  ],
  "ingredients": [
    "1tablespoon ground coriander",
    "3tablespoons ground cumin",
    "2teaspoons ground turmeric",
    "1\u00bdteaspoons freshly ground black pepper",
    "1teaspoon ground cardamom",
    "\u00bdteaspoon ground cloves"
  ],
  "directions": [
    "Combine all the ingredients in a small glass jar, seal, and store in a cool, dark place for up to 1 year."
  ]
}