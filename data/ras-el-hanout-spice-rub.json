{
  "title": "Ras el Hanout Spice Rub",
  "source": "The Chicken Bible - ATK",
  "servings": "Makes about \u00bd cup",
  "categories": [
    "The Chicken Bible - ATK"
  ],
  "ingredients": [
    "2tablespoons paprika",
    "4teaspoons ground coriander",
    "4teaspoons ground cumin",
    "1tablespoon packed brown sugar",
    "1teaspoon ground cardamom",
    "1teaspoon ground cinnamon",
    "\u00beteaspoon table salt",
    "\u00bdteaspoon ground cloves",
    "\u00bdteaspoon ground nutmeg",
    "\u00bdteaspoon cayenne pepper"
  ],
  "directions": [
    "Combine all ingredients in bowl. (Store leftover spice rub for up to 3 months.)"
  ]
}