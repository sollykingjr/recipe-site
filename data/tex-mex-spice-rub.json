{
  "title": "Tex-Mex Spice Rub",
  "source": "The Chicken Bible - ATK",
  "servings": "Makes about 1 cup",
  "categories": [
    "The Chicken Bible - ATK"
  ],
  "ingredients": [
    "\u00c2\u00bccup ground cumin",
    "2tablespoons chili powder",
    "2tablespoons ground coriander",
    "2tablespoons dried oregano",
    "2tablespoons garlic powder",
    "4teaspoons kosher salt",
    "2teaspoons unsweetened cocoa powder",
    "1teaspoon cayenne pepper"
  ],
  "directions": [
    "Combine all ingredients in bowl. (Store leftover spice rub for up to 3 months.)"
  ]
}